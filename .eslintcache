.data);\r\n    const loading = useSelector(state => state.loaitours.loading)\r\n    const dispatch = useDispatch();\r\n    const actionResult = async () => { await dispatch(loaitourData()) }\r\n\r\n    const history = useHistory()\r\n    const hangdleDelete = e => {\r\n        dispatch(removeloaitour(e));\r\n        setTimeout(() => {\r\n            actionResult();\r\n        }, 500);\r\n    }\r\n    const hangdleEdit = (id) => {\r\n        history.replace(`${match.url}/sualoaitour/${id}`)\r\n    }\r\n    const handleStatus = (e, id) => {\r\n        if (e === 1) {\r\n            dispatch(updateloaitour({ status: 0, idsua: id }))\r\n        } else {\r\n            dispatch(updateloaitour({ status: 1, idsua: id }))\r\n        }\r\n        setTimeout(() => {\r\n            actionResult();\r\n        }, 500);\r\n    }\r\n\r\n    return (\r\n        <div id=\"admin\">\r\n            <div className=\"heading\">\r\n                <h4>Loại tour</h4>\r\n                <div className=\"hr\"></div>\r\n            </div>\r\n            <div className=\"content\">\r\n                <div className=\"add\">\r\n                    <Link to={`${match.url}/themloaitour`}><Button variant=\"outlined\" color=\"secondary\"><i className=\"fas fa-plus\"></i>&nbsp;&nbsp; Thêm mới</Button></Link>\r\n                </div>\r\n                {loading ? <div className=\"spin\"><Spin className=\"mt-5\" /></div> :\r\n                    <Table columns={columns} dataSource={loaitours.map((ok, index) => (\r\n                        {\r\n                            key: index + 1,\r\n                            name: <span>{ok.name}</span>,\r\n                            status: <div className=\"action\">{ok.status === 1 ? <span onClick={() => { handleStatus(ok.status, ok.id) }}><i className=\"far fa-thumbs-up text-primary\"></i></span> : <span onClick={() => handleStatus(ok.status, ok.id)}><i className=\"far fa-thumbs-down \"></i></span>}</div>,\r\n                            action:\r\n                                <div className=\"action\">\r\n                                    <Popconfirm title=\"Bạn có muốn sửa？\" onConfirm={() => { hangdleEdit(ok.id) }} icon={<QuestionCircleOutlined style={{ color: 'green' }} />}>\r\n                                        <i className=\"far fa-edit mr-4\"></i>\r\n                                    </Popconfirm>\r\n                                    <Popconfirm title=\"Bạn có muốn xoá？\" onConfirm={() => { hangdleDelete(ok.id) }} icon={<QuestionCircleOutlined style={{ color: 'red' }} />}>\r\n                                        <i className=\"far fa-trash-alt\" ></i>\r\n                                    </Popconfirm>\r\n                                </div>\r\n                        }))}\r\n                    />\r\n                }\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nLoaitour.propTypes = {\r\n\r\n}\r\n\r\nexport default Loaitour","D:\\VSCode\\Project-guardian\\WebsiteDuLich\\FrontEnd\\src\\features\\container\\admin\\Loaitour\\Themloaitour.js",["533"],"D:\\VSCode\\Project-guardian\\WebsiteDuLich\\FrontEnd\\src\\features\\container\\admin\\Doanhthu\\Doanhthu.js",["534","535","536","537","538","539"],"D:\\VSCode\\Project-guardian\\WebsiteDuLich\\FrontEnd\\src\\api\\tourLoaitour.js",[],"D:\\VSCode\\Project-guardian\\WebsiteDuLich\\FrontEnd\\src\\api\\dichvuTourApi.js",[],"D:\\VSCode\\Project-guardian\\WebsiteDuLich\\FrontEnd\\src\\app\\userSlice.js",[],"D:\\VSCode\\Project-guardian\\WebsiteDuLich\\FrontEnd\\src\\features\\components\\Trangchu.js",[],"D:\\VSCode\\Project-guardian\\WebsiteDuLich\\FrontEnd\\src\\features\\components\\Admin.js",[],"D:\\VSCode\\Project-guardian\\WebsiteDuLich\\FrontEnd\\src\\features\\teststripe\\Stripe.js",["540"],"D:\\VSCode\\Project-guardian\\WebsiteDuLich\\FrontEnd\\src\\api\\inforApi.js",[],"D:\\VSCode\\Project-guardian\\WebsiteDuLich\\FrontEnd\\src\\features\\container\\login\\inforSlice.js",[],"D:\\VSCode\\Project-guardian\\WebsiteDuLich\\FrontEn